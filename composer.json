{
  "name": "zendframework/skeleton-module",
  "description": "Module skeleton to start new Zend Framework modules",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "framework",
    "mvc",
    "zf"
  ],
  "homepage": "http://framework.zend.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.9",
    "squizlabs/php_codesniffer": "^3.0",
    "zendframework/zend-modulemanager": "^2.7.2",
    "zendframework/zend-test": "^3.1",
    "zendframework/zend-mvc": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "ZendSkeletonModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "ZendSkeletonModuleTest\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
